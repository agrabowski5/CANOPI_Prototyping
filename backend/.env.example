# CANOPI Backend Configuration
# Copy this file to .env and fill in your actual values

# Database
DATABASE_URL=postgresql://canopi:canopi_dev_password@localhost:5432/canopi
TIMESCALE_URL=postgresql://timescale:timescale_dev_password@localhost:5433/timescale

# Redis
REDIS_URL=redis://localhost:6379/0

# Celery
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/0

# API Keys
MAPBOX_API_KEY=your_mapbox_api_key_here

# Gurobi
GUROBI_HOME=/opt/gurobi1100/linux64
GRB_LICENSE_FILE=/opt/gurobi/gurobi.lic

# Application
DEBUG=True
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_here_change_in_production
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# External Data Sources
CAISO_API_KEY=your_caiso_api_key
NOAA_API_KEY=your_noaa_api_key
EIA_API_KEY=your_eia_api_key
